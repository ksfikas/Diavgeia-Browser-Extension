{
    "name": "Diavgeia Chrome Extension",
    "version": "1.1",
    "description": "Unofficial diavgeia.gov.gr extension for various manipulations of the ADA number",
    "permissions": ["declarativeContent", "storage", "clipboardWrite", "clipboardRead", "notifications", "tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}